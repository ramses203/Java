package gameLevel;

public abstract class AdvanceLevel extends PlayerLevel{
	
	public void run() {
		System.out.println("빨리달립니다.");
	}
		public void jump() {
		System.out.println("점프합니다.");
	}
		public void turn() {
		System.out.println("턴할줄 모릅니다.");
	}
		public void showLevleMessage() {
		System.out.println("******중급자 레벨입니다.********");
	}


}
